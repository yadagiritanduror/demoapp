<% unless @keywords.nil? %>
  <table >
    <% @keywords.each do |keyword| %>
      <tr>
          <td>
                <%= keyword.keyword %>
          </td>
            <td> <%= link_to 'EDIT', edit_keyword_path(keyword), remote: true %></td>
            <td><%= link_to 'DELETE', keyword_path(keyword),remote: 'true', method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
    <% end %>
  </table>
<% end %>
